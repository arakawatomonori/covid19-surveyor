{
    "name": "covid19-surveyor",
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "../www-data/map" }
      }
    ],
    "routes": [
      {
        "src": "/(js|css|img)/.*",
        "headers": { "cache-control": "max-age=31536000, immutable" }
      },
      { "handle": "filesystem" },
      { "src": ".*", "dest": "/" }
    ]
  }